
Host default
  HostName 127.0.0.1
  User vagrant
  Port 2222
  UserKnownHostsFile /dev/null
  StrictHostKeyChecking no
  PasswordAuthentication no
  IdentityFile /Users/joeyb/.vagrant.d/insecure_private_key
  IdentitiesOnly yes


Host my_server
  Hostname my_server.com
  User my_username
  IdentityFile ~/.ssh/id_rsa
  IdentitiesOnly yes

